@startuml
!include https://raw.githubusercontent.com/plantuml-stdlib/C4-PlantUML/master/C4_Container.puml

title Context

Person(Customer, "Пользователь", "Пользователь приложения")
    
System(App, "Система управления ресурсами", "Позволяет пользователям сохранять ссылки и управлять ими")
    
Rel(Customer, App, "Сохраняет ресурсы")

System_Ext(Browser, "Браузер", "Браузер в котором пользователь ведет работу")

BiRel(App, Browser, "Получает открытые вкладки")

Rel(Customer, Browser, "Открывает и закрывает вкладки")

@enduml
